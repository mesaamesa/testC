{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug bisection",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bisection",
            "args": ["../Samples/A3.MMM", "../Samples/B3.MMM", "3"],
            "stopAtEntry": false,
            "cwd": ".",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "CMake: build"
        },
        {
            "name": "Debug qzExplicit",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/qzexplicit",
            "args": ["../Samples/A18.MMM", "../Samples/B18.MMM", "1", "18"],
            "stopAtEntry": false,
            "cwd": ".",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "CMake: build"
        }

    ]
}